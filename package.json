{
  "name": "attendance-backend",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "knex": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts",
    "migrate": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:latest",
    "migrate:make": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:make",
    "migrate:current": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:current",
    "migrate:rollback": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts migrate:rollback",
    "seed": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts seed:run",
    "seed:make": "ts-node ./node_modules/knex/bin/cli.js --knexfile=knexfile.ts seed:make",
    "reset-db": "npm run migrate:rollback --all && npm run migrate && npm run seed && npm start"
  },
  "dependencies": {
    "@types/knex": "^0.16.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.11.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}